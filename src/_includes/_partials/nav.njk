<nav id="mainNav" role="navigation">
  <h2 class="sr-only">Top level navigation menu</h2>
  <div>
    <div class="indicator"></div>
  </div>
    {%- for entry in collections.all | eleventyNavigation %}
      <a id="{{ entry.title }}" aria-label="{{ entry.description }}" class="transition" href="{{ entry.url }}"{% if entry.url == page.url %} aria-current="page"{% endif %}>{{ entry.title }}</a></li>
    {%- endfor %}
</nav>
{% set js %}
{% include './nav/script.js' %}
{% endset %}
<script>{{ js | jsmin | safe }}</script>