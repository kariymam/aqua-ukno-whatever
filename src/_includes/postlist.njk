<ol reversed class="postlist grid" style="counter-reset: start-from {{ (postslistCounter or postslist.length) + 1 }}">
	{% for post in postslist | reverse %}
	<li class="postlist-item{% if post.url == url %} postlist-item-active{% endif %} group relative block mb-16 mx-4 rounded-3xl">
    {% include "_partials/articleCard.njk" %}
		{% block content %}{% endblock %}
	</li>
	{% endfor %}
</ol>